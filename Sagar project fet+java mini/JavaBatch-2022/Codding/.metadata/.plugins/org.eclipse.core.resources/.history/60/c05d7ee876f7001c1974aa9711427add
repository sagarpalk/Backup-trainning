class Circle
{
	float pi=3.14f;//Instance
	
	float area(float r)
	{
		return pi*r*r;
	}
	void show(float ans)
	{
		System.out.println("Area of the circle is:"+ans);
	}
}
public class StaticMethodExample {
	public static void main(String[] args) {
		Circle c=new Circle();
		float res=c.area(2.3f);
		c.show(res);
	}
}
