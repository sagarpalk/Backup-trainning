class Circle
{
	static float pi=3.14f;//Instance
	
	static float area(float r)
	{
		return pi*r*r;
	}
	void show(float ans)
	{
		System.out.println("Area of the circle is:"+ans);
	}
}
public class StaticMethodExample {
	public static void main(String[] args) {
		//Circle c=new Circle();
		float res=Circle.area(2.3f);
//		c.show(res);
	}
}
